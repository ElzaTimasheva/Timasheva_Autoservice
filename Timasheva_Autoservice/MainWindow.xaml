<Window x:Class="Timasheva_Autoservice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Timasheva_Autoservice"
        mc:Ignorable="d"
        Title="Автосервис Тимашевой" Height="700" Width="800" 
        FontFamily="Tw Cen MT" 
        Icon="Resources/service_logo.png" MinHeight="420" MinWidth="740">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        
        <Grid Background="#FFFFFFE1"></Grid>
        <Image Source="Resources/service_logo.png" HorizontalAlignment="Left"></Image>
        <TextBlock Name="TBHeader" Text="Header" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered"
               NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="BtnBack" HorizontalAlignment="Right" Click="BtnBack_Click"></Button>
        
    </Grid>
</Window>
